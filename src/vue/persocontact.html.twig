{% extends "base.html.twig" %}

{% block title %} {{ parent() }} Contact {% endblock %}

{% block contenu %} 
    <div class="table-responsive">
    <table class="table">
    <thead>
    <tr>
    <th scope="col">Email</th>
    <th scope="col">Nom</th>
    <th scope="col">Message</th>
    </tr>
    </thead>
    <tbody>
    {% for contact in liste1 %}
        <tr>
            <td>{{contact.email}}</td>
            <td>{{contact.nom}}</td>
            <td>{{contact.message}}</td>
        </tr>
    {% endfor %}
    </tbody>
    </table>
    </div>

{% endblock %} 